{
    "Stuns": [
        {
            "Proto": "udp",
            "URI": "stun:netbird.<DOMAIN>:4478"
        }
    ],
    "Relay": {
        "Addresses": ["rels://netbird.<DOMAIN>:4478/relay"],
        "CredentialsTTL": "24h",
        "Secret": "<NB_AUTH_SECRET>"
    },
    "Signal": {
        "Proto": "https",
        "URI": "netbird.<DOMAIN>:4478"
    },
    "Datadir": "/var/lib/netbird",
    "DataStoreEncryptionKey": "<DATASTORE_KEY>",
    "EmbeddedIdP": {
        "Enabled": true,
        "Issuer": "https://netbird.<DOMAIN>:4478/oauth2",
        "DashboardRedirectURIs": [
            "https://netbird.<DOMAIN>/nb-auth",
            "https://netbird.<DOMAIN>/nb-silent-auth"
        ]
    }
}